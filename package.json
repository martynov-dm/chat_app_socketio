    "datauri": "^3.0.0",
    "date-fns": "^2.16.1",
    "dotenv": "^8.2.0",
    "joi-image-extension": "^1.0.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.18",
    "multer": "^1.4.2",
